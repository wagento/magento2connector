body{color:#41362f;font-weight:400;font-family:'Open Sans','Helvetica Neue',Helvetica,Arial,sans-serif;font-style:normal;font-size:14px}a,a:hover{color:#30aabc}.section{margin-bottom:20px;display:inline-block;width:100%}#order-information,.accordion,.accordion-group:last-child{margin-bottom:0}.section-title{padding:0 0 10px;color:#30aabc}.section-title .title,.section-title strong{text-align:left;font-size:14px;font-weight:600}.section-item-title,.section-item-title .title{font-size:14px;font-weight:600;margin-bottom:0}.table,.table-secondary td,.table-secondary th{padding:.5rem;font-size:14px;text-align:left;width:100%}.table-secondary tbody tr:nth-child(odd) td,.table-secondary tbody tr:nth-child(odd) th{background-color:#f1f1f1}.table-wrapper{overflow-x:auto;overflow-y:hidden;width:100%;-ms-overflow-style:-ms-autohiding-scrollbar;-webkit-overflow-scrolling:touch;-webkit-box-shadow:inset 0 0 50px -20px rgba(0,0,0,.57);-moz-box-shadow:inset 0 0 50px -20px rgba(0,0,0,.57);box-shadow:inset 0 0 50px -20px rgba(0,0,0,.57)}.order-billing-address,.order-billing-method,.order-comments-history,.order-history,.order-information,.order-payment-method{float:left;width:calc((100%) * .5 - 0px)}.order-subtotal-table tfoot{border-top:solid 1px #e3e3e3}.order-items-table td.col-product,.order-items-table td.col-sku{white-space:nowrap}